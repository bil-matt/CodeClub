fizz = 3 ##This allows the numbers to be easily changed
buzz = 5
number = 0
for i in range(100):
    number += 1
    if number % fizz == 0 and number % buzz ==0:
        print("FizzBuzz")
    elif number % fizz == 0: ## % means divide that number by {number} and then output the remainder, if remainder = 0 its a multiple of it
        print("Fizz")
    elif number % buzz == 0:
        print("Buzz")
    else:
        print(number)
